---
import Check from "@src/assets/icon-svg/check.svg";

interface Props {
  title: string;
}
const { title } = Astro.props as Props;
---

<div class="flex gap-3 items-center justify-center flex-grow basis-0">
  <Check class="size-10 text-white drop-shadow-[2px_2px_1px_rgba(0,0,0,0.5)]" />
  <span class="text-2xl text-black font-semibold">{title}</span>
</div>
